{
  "name": "@teleporthq/teleport-registry-packer",
  "version": "0.1.0",
  "description": "A opionated bundler for serving packages from npm",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"npm run local:deploy\" --onFailure \"echo Compilation Failed\" --noClear",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "local:deploy": "functions-framework --source ./dist/index.js --target=app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teleporthq/teleport-registry-packer.git"
  },
  "author": "teleportHQ",
  "homepage": "https://teleporthq.io/",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@google-cloud/storage": "^3.0.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "etag": "^1.8.0",
    "node-fetch": "^2.6.0",
    "npm": "^6.14.8",
    "request": "^2.81.0",
    "request-promise": "^4.2.6",
    "rollup": "^2.26.6",
    "rollup-plugin-esm-import-to-url": "^2.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "semver": "^5.3.0",
    "sha1": "^1.1.1",
    "tar": "^4.4.8"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/babel__core": "^7.1.9",
    "@types/etag": "^1.8.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/request": "^2.48.5",
    "@types/semver": "^7.3.3",
    "@types/sha1": "^1.1.2",
    "@types/tar": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
  }
}
