{
  "name": "@teleporthq/teleport-registry-packer",
  "version": "0.3.0",
  "description": "A opionated bundler for serving packages from npm",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "dev": "ts-node-dev ./src/index.ts",
    "clean": "rimraf dist",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "postbuild": "mkdir dist/public && cp -r public dist/",
    "local:deploy": "functions-framework --source ./dist/index.js --target=app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JayaKrishnaNamburu/teleport-registry-packer.git"
  },
  "author": "teleportHQ",
  "homepage": "https://teleporthq.io/",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@google-cloud/storage": "^3.0.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression": "^1.7.0",
    "cors": "^2.8.5",
    "etag": "^1.8.0",
    "express": "^4.15.2",
    "lru-cache": "^5.1.1",
    "minilog": "^3.1.0",
    "node-fetch": "^2.6.0",
    "npm": "^6.6.0",
    "pretty-bytes": "^5.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.6",
    "rollup": "^2.26.6",
    "rollup-plugin-esm-import-to-url": "^2.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "semver": "^5.3.0",
    "serve-favicon": "^2.5.0",
    "sha1": "^1.1.1",
    "tar": "^4.4.8",
    "terser": "^3.14.1"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/babel__core": "^7.1.9",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/etag": "^1.8.0",
    "@types/express": "^4.17.7",
    "@types/lru-cache": "^5.1.0",
    "@types/minilog": "^2.0.29",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/request": "^2.48.5",
    "@types/semver": "^7.3.3",
    "@types/serve-favicon": "^2.5.0",
    "@types/sha1": "^1.1.2",
    "@types/tar": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "chokidar": "^3.4.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^3.9.7"
  }
}
